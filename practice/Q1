#include <bits/stdc++.h>
//without using the euclidean algorithm just to practice
using namespace std;

int gcdLogical(int a, int b) {
    int min = a<b?a:b, g=1;
    for(int i=2;i<=min;i++) {
        if(a%i==0 && b%i==0) g=i;
    }
    return g;
}

int gcdEuclidean(int a, int b) {
    if (b==0) return a;
    return gcdEuclidean(b, a%b);
}

int main() {
    int a, b;
    cout << "enter 2 numbers: " << endl;
    cin >> a >> b;
    int g1 = gcdLogical(a, b);
    cout << "gcd of " << a << " and " << b << " without using Euclidean algorithm is: " << g1 << endl;
    int g2 = gcdEuclidean(a, b);
    cout << "gcd of " << a << " and " << b << " using Euclidean algorithm is: " << g2 << endl;
    return 0;
}